{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material/button": "^2.3.0",
    "editorconfig": "^0.15.3",
    "lint": "^1.1.2",
    "material-icons": "^0.3.1",
    "material-ui": "^0.20.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:c": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "react/jsx-indent": [
        "error",
        2
      ],
      "import/prefer-default-export": "off"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true,
        "classes": true
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^2.3.0"
  },
  "husky": {
    "hooks": {}
  }
}
